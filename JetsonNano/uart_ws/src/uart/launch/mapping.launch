<launch>
  <node pkg="tf" type="static_transform_publisher" name="base_link_to_laser" args="0.0 0.0 0.25 0.0 0.0 0.0 /base_link /laser 40" />
  <node pkg="tf" type="static_transform_publisher" name="imu_broadcaster" args="0 0 0 0 0 0 base_link imu 30" />
  <include file="$(find rplidar_ros)/launch/rplidar.launch" />
  <include file="$(find uart)/launch/scanmatcher.launch" />
  
  <node pkg="gmapping" name="slam_gmapping" type="slam_gmapping" output="screen">
    <param name="delta" value="0.1"/>
    <param name="xmin" value="-2"/>
    <param name="xmax" value="2"/>
    <param name="ymin" value="-2"/>
    <param name="ymax" value="2"/>
    <param name="base_frame" value="base_link" />
    <param name="map_udpate_interval" value="1.0"/>
    <param name="linearUpdate" value="0.5"/>
    <param name="angularUpdate" value="0.436"/>
    <param name="temporalUpdate" value="-1.0"/>
    <param name="resampleThreshold" value="0.5"/>
    <param name="particles" value="80"/>
    <!--param name="odom_frame" value="robot_1/odom"/-->
  </node>
  
  <node name="listenuart" pkg="uart"  type="listen_and_uart" />
    <!-- <include file="$(find uart)/launch/movebase.launch" /> -->
  <!-- move_base -->
  
  
</launch>